#include<bits/stdc++.h>
using namespace std;
#define rep(i,m,n) for(int i=m;i<n;i++)
#define ll long long
#define pb push_back
#define ull unsigned long long
int main()
{
    vector<ull>nums;
    ull num;
    while(cin>>num && num!=EOF) nums.pb(num);
    for(int i=nums.size()-1;i>=0;i--) printf("%.4f\n",sqrt(nums[i]));
}
